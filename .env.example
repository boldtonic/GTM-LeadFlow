# Copy this file to .env and fill in your API keys

# Required: Google Places API
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_PLACES_API_KEY=your_google_places_api_key

# Optional: Apollo.io for company enrichment
# Get from: https://app.apollo.io/#/settings/integrations/api
APOLLO_API_KEY=your_apollo_api_key

# Optional: Hunter.io for email finding
# Get from: https://hunter.io/api
HUNTER_API_KEY=

# Optional: Firecrawl for enhanced web scraping
# Get from: https://firecrawl.dev
FIRECRAWL_API_KEY=your_firecrawl_api_key

# Optional: OpenAI for AI-powered GTM Brief generation
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key
